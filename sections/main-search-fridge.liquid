  // Search Bar main


  

  
</script>

<style>
  .product-listing-section .search_pagination {
    margin-top: 5rem;
    margin-bottom: 10rem;
    display: initial;
}
 .product-listing-section  .pagination__list>li {
    flex: 1 0 4.4rem;
    max-width: 4.4rem;
}
 .product-listing-section  .pagination__item {
    color: rgb(var(--color-foreground));
    display: inline-flex;
    justify-content: center;
    align-items: center;
    position: relative;
    height: 4.4rem;
    width: 100%;
    padding: 0;
    text-decoration: none;
    font-size:1.6rem;
    font-family: "SamsungOne 700";
}
 .product-listing-section  ul.pagination__list.list-unstyled {
    list-style: none;
    display: flex;
    text-align: center;
    justify-content: center;
}
 .product-listing-section  .pagination__item--current:after, .pagination__item:hover:after {
    content: "";
    display: block;
    width: 2rem;
    position: absolute;
    bottom: 8px;
    left: 50%;
    transform: translate(-50%);
    background-color: currentColor;
    height: 2px;
}
  
 .product-listing-section .filter-loading {
    background: #16141473;
       top: -30px;
       height: 98.5%;
} 
  .product-listing-section {
    position: relative;
}
  
  .product-listing-section .loader-holder {
    top: 20%;
  }

 .search_pagination {
    display: flex;
    flex-wrap: wrap;
}

.search_pagination li:nth-child(n+7) {
    display: none;
}

.search_pagination li:last-child 
{
    display: block;
}
.pagination__item-arrow {
    background: #1428a0!important;
    color: #fff!important;
}
  .pagination__item-arrow:after{
  display:none!important;
  }

.pagination-next  .icon {
    margin-right: -0.2rem;
    transform: rotate(-90deg);
  margin-top: 18px;
}
.pagination__item .icon-caret {
    height: 0.6rem;
}
  
.pagination-prev .icon {
    margin-left: -0.2rem;
    transform: rotate(90deg);
}
 
  
</style>





{% schema %}
  {
    "name": "Main Search Fridge",
   "class" : "search-fridge-mainn", 
    "settings": [
       

        {
        "id": "title",
        "type": "text",
        "label": "Heading"
        },
        {
        "id": "title1",
        "type": "text",
        "label": "Sub Heading", 
        "default" : "Refine your search by part type"
        },
          {
        "id": "title3",
        "type": "text",
        "label": "Heading Search Diagram", 
        "default" : "Search using diagrams"
        } , 
         {
        "id": "title2",
        "type": "html",
        "label": "Heading Search Within", 
        "default" : "Search <span>within</span> RF28K9071257"
        }   

    ],
    "blocks": [
  {
    "type": "slider",
    "name": "Slider",
    "settings": [
      {
        "id": "banner_img",
        "type": "image_picker",
        "label": "Slider Image"
      },
      {
      "id": "image_sub_title",
      "type": "text",
      "label": "Name ", 
      "default" : "Name"
      } 
    ]
  }
]
  }
{% endschema %}

{% stylesheet %}
.carousel-search-diagrams .items-inner {
    margin: 8px;
 text-align:center;
}
.carousel-search-diagrams .slick-arrow {
    width: 15px;
    height: 26px;
}
.carousel-search-diagrams .slick-arrow:before{
display:none
}
.carousel-search-diagrams .slick-arrow {
    background-image: url(https://cdn.shopify.com/s/files/1/0601/8488/7543/files/arrow-right.png?v=1640847533);
   background-repeat: no-repeat;
}
.carousel-search-diagrams button.slick-prev.slick-arrow {
    transform: rotate(180deg);
}






{% endstylesheet %}

