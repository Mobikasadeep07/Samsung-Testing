<div class="search-fridge-main">
 
  <h1 class="search-res-head searched-result" style="display:none;">
<!--     {{ section.settings.title }}  -->
    <span class="diagram-count">87</span> results for <span class="search-query-diagram">RF28K9070SG</span>
  </h1>   
 
  
  {% comment %}
   <h2> {{ section.settings.title1 }}</h2>
  <div class="part-type">
   
        
          
          <label class="texture-swatch">
            
            <input type="radio" name="properties[part_type]" value="Self">
            
            <span class="tooltip-text">Self</span>
          </label>	
    
           <label class="texture-swatch">
            
            <input type="radio" name="properties[part_type]" value="Bin">
            
            <span class="tooltip-text">Bin</span>
          </label>	
          <label class="texture-swatch">
            
            <input type="radio" name="properties[part_type]" value="Water Filter">
            
            <span class="tooltip-text">Water Filter</span>
          </label>	
    	  <label class="texture-swatch">
            
            <input type="radio" name="properties[part_type]" value="Exterior Parts">
            
            <span class="tooltip-text">Exterior Parts</span>
          </label>	
    
          <label class="texture-swatch">
            
            <input type="radio" name="properties[part_type]" value="Screws">
            
            <span class="tooltip-text">Screws</span>
          </label>	
    
           <label class="texture-swatch">
            
            <input type="radio" name="properties[part_type]" value="Electronics">
            
            <span class="tooltip-text">Electronics</span>
          </label>	
          <label class="texture-swatch">
            
            <input type="radio" name="properties[part_type]" value="Hardware">
            
            <span class="tooltip-text">Hardware</span>
          </label>	
    	  <label class="texture-swatch">
            
            <input type="radio" name="properties[part_type]" value="Others">
            
            <span class="tooltip-text">Others</span>
          </label>	
      
  </div>
  {% endcomment %}
  
  <div class="search-with-diagram">
    <h2> {{ section.settings.title3 }} </h2>
    
    <section class="banner-section">
       {% comment %}
      <div class="carousel-search-diagrams">
        {% for block in section.blocks %}
        <div class="item">
          <div class="items-inner">
            <img src="{{ block.settings.banner_img | img_url : 'master' }}" alt="">
            <span>{{ block.settings.image_sub_title }}</span>
          </div>
        </div>        
        {% endfor %}  
      </div>
       {% endcomment %}
      <div class="diagram-img-api carousel-search-diagrams">

      </div>

    </section>
    
  </div>
  
  
  
  
  <div class="search-within search_model_number ">
      <h2 class="heading-within" style="display:none;">
<!--         {{ section.settings.title2 }}  -->
        Search <span>within</span> <span class="search-query-diagram"></span>
      </h2>   
  
    <div class="search-form-results search-page-results">
    <p class="search-res-heading">Search within these results</p>
   <form action="" method="get" role="search" class="find-search-form">  
      
    <input class="search-input-bx" id="search-page-with-data" type="text" name="q1" placeholder="Search" aria-label="Search" autocomplete="off">
    <button type="submit" class="btn submit"><img src="//cdn.shopify.com/s/files/1/0601/8488/7543/t/5/assets/icon_bold.png?v=14985898541735511435" alt=""> </button>
  </form>
      <div id="cate-col-data" class="usf-popup usf-zone usf-is usf-is--left">
      <div class="usf-is__content">
        <div class="usf-is__matches usf-is__products">
          <div class="usf-title">Product matches</div> 
          <div class="usf-is__list"><!----> 
            <span class="usf-is__product usf-clear">
              <div class="usf-img-wrapper usf-pull-left">
                <img src="//cdn.shopify.com/s/files/1/0257/0108/9360/t/85/assets/no-image.svg" class="usf-img"></div> 
              <div class="usf-pull-left"><div class="usf-title"><span class="usf-highlight">04</span>02-001296</div> 
                <div class="usf-vendor"></div> <div class="usf-price-wrapper"><span class="usf-price">$4</span>
                <!----></div></div>
            </span>
            <span class="usf-is__product usf-clear">
            <div class="usf-img-wrapper usf-pull-left">
              <img src="//cdn.shopify.com/s/files/1/0257/0108/9360/t/85/assets/no-image.svg" class="usf-img">
            </div> 
            <div class="usf-pull-left">
              <div class="usf-title"><span class="usf-highlight">04</span>06-001293</div> 
              <div class="usf-vendor"></div> <div class="usf-price-wrapper">
              <span class="usf-price">$2</span> <!----></div></div></span>
          </div>
          <span class="nodata"></span>
        </div>
         </div>
      
        </div>
      
    </div>
    <div class="sort-right-results">
      <p>Filter results by part type</p>
    <select class="sort-by" id="part_type_options">
      <option value="Select a part type">Select a part type</option>
    </select>
    </div>
    
    <div class="sort-right-align">
      <label><b>Sort By</b></label>
    <select class="sort-by" id="sort-by">
      <option value="">Relevance</option>
      <option value="">Title A-Z</option>
      <option value="">Title Z-A</option> 
      
      <option value="">Date: Old to New</option>
      <option value="">Date: New to Old</option> 
      <option value="">Price: Low to High</option> 
      <option value="">Price: High to Low</option> 
    </select>
    </div>
  </div>
  
  <div class="product-listing-section">
  	<div class="inner-section">
    </div>
    <a href="#" class="btn" id="seeMore">Load More</a>
    <div class="pagination-wrapper search_pagination"><nav class="pagination"><ul class="pagination__list list-unstyled"></ul></nav></div>
    <div class="filter-loading">
    <div class="loader-holder" id="loader-holder">
      <div class="loading"></div>
      <label>loading</label>
  </div>
    </div>
  </div>
  
</div>



